node{
    for(int i=0;i<2;i++){
        
        stage "stage #"+ i
        print 'Changes in jenkins build file'
        if(i==0){
            git "https://github.com/nishsapkal/startup/devops-demo"
            echo 'running on stage 0'
        }else{
            build 'declarative pipeline'
            echo 'running on stage 1'
        }
        
    }
}