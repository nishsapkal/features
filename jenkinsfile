node{
    for(int i=0;i<2;i++){
        
        stage "stage #"+ i
        print 'Changes in jenkins build file'
        if(i==0){
          //  url: 'https://github.com/nishsapkal/startup/',
			//branch: "devops-demo"
            echo 'running on stage 0'
        }else{
            build 'nehr-pipeline'
            echo 'running on stage 1'
        }
        
    }
}